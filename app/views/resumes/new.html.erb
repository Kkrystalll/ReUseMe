<h1>新增履歷</h1>

<%= form_with model:@resume do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>
  <div>
    <%= form.label :content %>
    <%= form.text_area :content %>
  </div>
  <div>
    <%= form.label :status %>
    <%= form.select :status, Resume.all_status %>
  </div>
  <%= form.submit "新增" %>
<% end %>